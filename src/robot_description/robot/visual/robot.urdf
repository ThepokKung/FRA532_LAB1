<?xml version="1.0"?>
<robot name="fake-limo">
    <!-- base link -->
    <link name="base_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="1.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
    </link>

    <!-- chassis link -->
    <joint name="chassis_joint" type="fixed">
        <origin xyz="0.0 0.0 0.095" rpy="0.0 0.0 1.57"/>
        <parent link="base_link"/>
        <child link="chassis"/>
    </joint>

    <link name="chassis">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="3.0"/>
            <inertia ixx="0.1" ixy="0.0" ixz="0.0" iyy="0.1" iyz="0.0" izz="0.1"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.13 0.20 0.1"/>
            </geometry>
            <material name="">
                <color rgba="1.0 1.0 1.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <box size="0.13 0.20 0.1"/>
            </geometry>
        </collision>
    </link>

    <!-- front left wheel -->
    <joint name="front_left_wheel_joint" type="revolute">
        <origin xyz="0.10 0.07 0.045" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="front_left_wheel_joint_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.7854" upper="0.7854" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="front_left_wheel_joint_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
    </link>

    <joint name="front_left_wheel" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="-1.57 0.0 0.0"/>
        <parent link="front_left_wheel_joint_link"/>
        <child link="front_left_wheel_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="front_left_wheel_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
        <visual name="">
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                <cylinder radius="0.045" length="0.01"/>
            </geometry>
            <material name="">
                <color rgba="0.0 0.0 0.0 1.0"/>
                <texture filename=""/>
            </material>
        </visual>
        <collision>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <geometry>
                 <cylinder radius="0.045" length="0.01"/>
            </geometry>
        </collision>
    </link>

    <!-- front right wheel -->
    <joint name="front_right_wheel_joint" type="revolute">
        <origin xyz="0.10 -0.07 0.045" rpy="0.0 0.0 0.0"/>
        <parent link="base_link"/>
        <child link="front_right_wheel_joint_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="-0.7854" upper="0.7854" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="front_right_wheel_joint_link">
        <inertial>
            <origin xyz="0.0 0.0 0.0" rpy="0.0 0.0 0.0"/>
            <mass value="0.1"/>
            <inertia ixx="0.01" ixy="0.0" ixz="0.0" iyy="0.01" iyz="0.0" izz="0.01"/>
        </inertial>
    </link>

    <joint name="front_right_wheel" type="continuous">
        <origin xyz="0.0 0.0 0.0" rpy="-1.57 0.0 0.0"/>
        <parent link="front_right_wheel_joint_link"/>
        <child link="front_right_wheel_link"/>
        <axis xyz="0.0 0.0 1.0"/>
        <limit lower="0.0" upper="0.0" effort="0.0" velocity="0.0"/>
    </joint>

    <link name="front_right_wheel_link">
        <inertial>
